{
  "name": "nettoweb/laravel-cms-currency",
  "description": "Currency support for Nettoweb content management system",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Netto\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Alexander Sidorenkov",
      "email": "contact@nettoweb.ru"
    }
  ],
  "require": {
      "nettoweb/laravel-cms": "^1.2",
      "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
